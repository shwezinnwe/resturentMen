	<script src="../js/jquery.min.js"></script>
    <script src="../js/bootstrap.min.js"></script>
    <script src="../js/process.js"></script>
</head>

<style>
	.product-status{
		padding-left: 10px;
		padding-top: 100px;
		padding-bottom: 50px;
	}

	.paddingLR{
		padding-left: 20px;
		padding-right: 20px;
	}

	.btn-yellow{
		color: gray;
	}

</style>

<body>
	<div class="container-fluid">
		<div class="row">
			<div class="col-lg-2 col-md-2 paddingReset">
			    <div class="left-sidebar-pro">
					<nav id="sidebar" class="">

						<ul class="myScroll">
							<li>
								<div class="sidebar-header">
									<a title="Home" href="/"><img class="main-logo" src="../img/logo/logo13.png" alt="" /></a>
								</div>
							</li>

							<li>
								<div class="left-custom-menu-adp-wrap comment-scrollbar">
									<nav class="sidebar-nav left-sidebar-menu-pro">

										<ul class="metismenu" id="menu1">

												<% include ../partial/navbar %>

										</ul>
									</nav>
								</div>
							</li>
						</ul>

			        </nav>
			    </div>
			</div>

			<div class="col-lg-10 col-md-10 col-sm-12 col-xs-12 paddingReset">

				    <div class="all-content-wrapper">
				        <div class="header-advance-area">
				            <div class="header-top-area">
				                <div class="container-fluid">
				                    <div class="row">

				                        <div class="col-lg-12 col-md-12 col-sm-11 col-xs-12">
				                            <div class="header-top-wraper">
				                                <div class="row marginReset">

	                                				<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 paddingReset">
                                						<div class="dropdown">
                                							<a class="btn dropdown-toggle" data-toggle="dropdown">
                                								<span class="admin-name"><i class="fa fa-bars"></i></span>
                                							</a>

                                							<ul id="listdown" role="menu" class="dropdown-menu animated zoomIn myScroll1">

																	<% include ../partial/navbar %>

                                							</ul>
                                						</div>
	                                				</div>

																					<div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 paddingReset">
																							<div class="dropdown">
																									<a data-toggle="dropdown" class="btn dropdown-toggle">
																						<i class="fa fa-user adminpro-user-rounded header-riht-inf" aria-hidden="true"></i>
																						<span class="admin-name"><%= name %></span>
																						<i class="fa fa-angle-down adminpro-icon adminpro-down-arrow"></i>
																								</a>

																									<ul class="dropdown-menu animated zoomIn">


																											<li><a href="/logout"><span class="fa fa-lock author-log-ic"></span>Log Out</a>
																											</li>
																									</ul>
																							</div>
																					</div>

				                                </div>
				                            </div>
				                        </div>
				                    </div>
				                </div>
				            </div>

				            <div class="breadcome-area">
				                <div class="container-fluid">
				                    <div class="row">
				                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
				                            <div class="breadcome-list">
				                                <div class="row">

				                                </div>
				                            </div>
				                        </div>
				                    </div>
				                </div>
				            </div>
				        </div>

				        <div class="product-status">
				            <div class="container-fluid">


													<div class="col-md-12">

	 													<div class="row">
	 				                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
	 				                            <div class="admin-content analysis-progrebar-ctn res-mg-t-30">
	 				                                <h4 class="text-left text-uppercase">Total Staff</h4>
	 				                                <div class="row vertical-center-box vertical-center-box-tablet">

	 				                                </div>
	 				                                 <div class="progress progress-mini">
	 				                                    <div style="width: 100%;" class="progress-bar bg-red"></div>

	 				                                </div>
																					<h3 class="text-right"><%= staff %></h3>
	 				                            </div>
	 				                        </div>

	 				                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
	 				                            <div class="admin-content analysis-progrebar-ctn res-mg-t-30">
	 				                                <h4 class="text-left text-uppercase"><b>Total Menu</b></h4>
	 				                                <div class="row vertical-center-box vertical-center-box-tablet">

	 				                                </div>
	 				                                <div class="progress progress-mini">
	 				                                    <div style="width: 100%;" class="progress-bar bg-purple"></div>
	 				                                </div>
																					<h3 class="text-right"><%= menu %></h3>
	 				                            </div>
	 				                        </div>

	 				                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
	 				                            <div class="admin-content analysis-progrebar-ctn res-mg-t-30">
	 				                                <h4 class="text-left text-uppercase"><b>Total Drinks</b></h4>
	 				                                <div class="row vertical-center-box vertical-center-box-tablet">

	 				                                </div>
	 				                                 <div class="progress progress-mini">
	 				                                    <div style="width: 100%;" class="progress-bar bg-green"></div>
	 				                                </div>
																					<h3 class="text-right"><%= drink %></h3>
	 				                            </div>
	 				                        </div>

	 										<div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
	 				                            <div class="admin-content analysis-progrebar-ctn res-mg-t-30">
	 				                                <h4 class="text-left text-uppercase"><b>Total Orders</b></h4>
	 				                                <div class="row vertical-center-box vertical-center-box-tablet">

	 				                                </div>
	 				                                <div class="progress progress-mini">
	 				                                    <div style="width: 100%;" class="progress-bar bg-orange"></div>
	 				                                </div>
																					<h3 class="text-right"><%= order %></h3>
	 				                            </div>
	 				                        </div>

	 				                    </div>
	 				                </div>
	                    	</div>
	                    </div>

				        <div class="footer footer-copyright-area">
				        	<div class="footer-copy-right">
				        		<p>Copyright &copy; 2018 <a href="https://colorlib.com/wp/templates/">Colorlib</a> All rights reserved.</p>
				        	</div>
				        </div>

				    </div>

			</div>
		</div>
	</div>

</body>
</html>
