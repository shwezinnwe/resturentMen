<script>
	document.getElementById('voucher').classList.add("hide");

	function myFunction(clickedID){
		document.getElementById('voucher').classList.toggle("hide");
		var c1 = document.getElementById('custom');
		var cf = document.getElementById('customF');
		var cd = document.getElementById('customD');
		var ct = document.getElementById('customT');

		c1.innerHTML = c1.innerText +" "+ clickedID;

		var l1 = document.createElement("li");
		var t1 = document.createTextNode("Grilled Duck : 15000");
		l1.appendChild(t1);

		var l2 = document.createElement("li");
		var t2 = document.createTextNode("Coconut Noodle : 1000");
		l2.appendChild(t2);

		cf.appendChild(l1);
		cf.appendChild(l2);

		var l3 = document.createElement("li");
		var t3 = document.createTextNode("Apple Juice : 2000");
		l3.appendChild(t3);

		var l4 = document.createElement("li");
		var t4 = document.createTextNode("Mocha : 1500");
		l4.appendChild(t4);

		cd.appendChild(l3);
		cd.appendChild(l4);

		ct.innerHTML = "Total : 19500";

		// {{!-- var l5 = document.createElement("li");
		// var t5 = document.createTextNode("Total : 19500");
		// l5.appendChild(t5);

		// ct.appendChild(l5); --}}
	}

	function clearAll(){
		var c1 = document.getElementById('custom');
		var cf = document.getElementById('customF');
		var cd = document.getElementById('customD');
		var ct = document.getElementById('customT');

		c1.innerHTML = "Voucher For Table";
		cf.innerHTML = "";
		cd.innerHTML = "";
		ct.innerHTML = "";	

		document.getElementById('voucher').classList.add("hide");

	}

